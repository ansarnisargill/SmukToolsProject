@page
@model SmukToolsApp.Pages.Projects.DetailsModel

@{
    ViewData["Title"] = "Details";
}
<div class="container">

<h1> Project Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Project Title
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Project.title)
        </dd>
        <dt class="col-sm-2">
            Is Complete?
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Project.iscomplete)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" class="btn-sm btn-outline-dark" asp-route-id="@Model.Project.id">Edit</a> |
    <a asp-page="./Index" class="btn-sm btn-outline-primary">Back to List</a>
</div>
</div>